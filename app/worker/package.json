{
    "name": "@vdtn359/news-worker",
    "version": "0.0.0",
    "private": true,
    "description": "news worker",
    "scripts": {
        "start:news": "ts-node -C ttypescript -T -P tsconfig.dev.json --require tsconfig-paths/register src/news-worker.ts",
        "start:stats": "ts-node -C ttypescript -T -P tsconfig.dev.json --require tsconfig-paths/register src/news-stats-worker.ts",
        "start": "run-p start:*",
        "script": "ts-node -C ttypescript -T -P tsconfig.dev.json --require tsconfig-paths/register",
        "build": "ttsc -P tsconfig.build.json"
    },
    "dependencies": {
        "@vdtn359/news-utils": "workspace:*",
        "@vdtn359/news-core": "workspace:*",
        "@vdtn359/news-models": "workspace:*",
        "@vdtn359/news-schema": "workspace:*",
        "@vdtn359/news-search": "workspace:*",
        "@vdtn359/news-sources": "workspace:*",
        "config": "3.3.1",
        "dotenv": "8.2.0",
        "ioredis": "4.16.0",
        "rxjs": "6.5.4",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@types/config": "0.0.36",
        "@types/ioredis": "4.14.9",
        "npm-run-all": "4.1.5",
        "ts-node": "8.8.1",
        "ttypescript": "1.5.10",
        "webpack": "4.42.1"
    }
}
